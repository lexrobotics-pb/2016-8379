<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:keepScreenOn="true"
  android:paddingLeft="@dimen/activity_horizontal_margin"
  android:paddingRight="@dimen/activity_horizontal_margin"
  android:paddingTop="@dimen/activity_vertical_margin"
  android:paddingBottom="@dimen/activity_vertical_margin"
  tools:context="com.qualcomm.ftccommon.ViewLogsActivity">

  <Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="2dp"
    android:layout_marginLeft="2dp"
    android:layout_marginStart="2dp"
    android:layout_marginTop="2dp"
    android:layout_margin="2dp"
    android:id="@+id/buttonClearLogs"
    android:onClick="onClickClearLogs"
    android:text="@string/button_text_clear_logs"
    android:textSize="12sp"/>
  <ScrollView
    android:id="@+id/scrollView"
    android:layout_below="@id/buttonClearLogs"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
      android:id="@+id/textAdbLogs"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:textSize="12sp"/>
    </ScrollView>
</RelativeLayout>
